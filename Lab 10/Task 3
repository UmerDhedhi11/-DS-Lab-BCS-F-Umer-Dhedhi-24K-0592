#include <iostream>
using namespace std;

class MaxHeap{
	public:
	    int t, i, l, r, largest, root, size;
	    int arr[1000];
	
	    MaxHeap(){
	        size = 0;
	    }
	    void insertKey(int x){
	        size++;
	        i = size;
	        arr[i] = x;
	        while(i>1 && arr[i/2]<arr[i]){
	            t = arr[i];
	            arr[i] = arr[i/2];
	            arr[i/2] = t;
	            i = i/2;
	        }
	    }
	    int extractMax(){
	        root = arr[1];
	        arr[1] = arr[size];
	        size--;
	        heapify(1);
	        return root;
	    }
	
	    void heapify(int i){
	        largest = i;
	        l = 2*i;
	        r = 2*i + 1;
	        if(l<=size && arr[l]>arr[largest]){
				largest = l;
			}
	        if(r<=size && arr[r]>arr[largest]){
				largest = r;
			}
	        if(largest != i){
	            t = arr[i];
	            arr[i] = arr[largest];
	            arr[largest] = t;
	            heapify(largest);
	        }
	    }
};

class KthLargest{
	private:
		int i, x;
	public:
	    int findKth(int arr[], int n, int k){
	        MaxHeap h;
	        for(i=0;i<n;i++){
				h.insertKey(arr[i]);
	        }
			x = 0;
	        for(i=0;i<k;i++){
				x = h.extractMax();
	        }
			return x;
	    }
};

int main(){
    int arr[] = {7, 49, 11, 10, 33, 21, 56, 89, 63, 3};
    int n = 10;
    int k = 5;
    KthLargest obj;
    cout << obj.findKth(arr, n, k);
    return 0;
}
